{
  "name": "talleu/php-redis-om",
  "description": "A PHP object mapper library for Redis",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Talleu\\RedisOm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Talleu\\RedisOm\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "symfony/property-access": "^7.1"
  },
  "suggest": {
    "ext-redis": "To use the php extension for Redis (phpredis)",
    "predis/predis": "To use Predis as a Redis PHP client"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "friendsofphp/php-cs-fixer": "^3.57",
    "phpstan/phpstan": "^1.11",
    "symfony/var-dumper": "^7.0"
  },
  "license": "MIT",
  "bin": ["bin/redisMigration"],
  "authors": [
    {
      "name": "Cl√©ment Talleu",
      "email": "clement.talleu@gmail.com"
    }
  ]
}
